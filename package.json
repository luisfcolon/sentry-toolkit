{
  "name": "sentry-toolkit",
  "version": "1.0.0",
  "description": "Sentry tools and logger",
  "main": "dist",
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 src --ignore .test. --out-dir ./dist",
    "test": "./node_modules/jest/bin/jest.js --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luisfcolon/sentry-toolkit.git"
  },
  "author": "Luis Colon <luiscolon62@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luisfcolon/sentry-toolkit/issues"
  },
  "homepage": "https://github.com/luisfcolon/sentry-toolkit#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/sentry/index.{js}",
      "!src/index.{js}"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "jest": "^21.1.0",
    "ohas": "^1.0.1",
    "raven-js": "^3.17.0",
    "sinon": "^3.3.0"
  }
}
